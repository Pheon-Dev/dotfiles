###############################################################
# A skin for a terminal with a dark background
# This skin uses RGB values so won't work for some
# terminals.
# https://www.rapidtables.com/convert/color/hex-to-rgb.html
# Created by Majixed
# Based on the catppuccin-mocha theme by A. Taha Baki <atahabaki@pm.me>
#
# Doc at https://dystroy.org/broot/skins/
###############################################################

skin: {
    input: rgb(192, 202, 245) none
    # fg:none bg:$surface2
    selected_line: none rgb(40, 42, 54) 
    # fg:$text bg:none
    default: rgb(192, 202, 245) none
    # fg:$overlay0 bg:none
    tree: rgb(68, 71, 90) none
    # fg:$sapphire bg:none
    parent: rgb(130, 170, 255) none
    file: rgb(192, 202, 245) none
#
### PERMISSIONS
#
    perm__: rgb(184, 192, 224) none
    # $peach
    perm_r: rgb(247, 144, 0) none
    # $maroon
    perm_w: rgb(189, 147, 249) none
    # $green
    perm_x: rgb(169, 255, 104) none
    # $teal
    group: rgb(241, 250, 140) none
    # $sky
    owner: rgb(137, 221, 255) none
#
### DATE
#
    # $subtext1
    dates: rgb(184, 192, 224) none
#
### DIRECTORY
#
    # $lavender
    directory: rgb(138, 162, 247) none Bold
    # $green
    exe: rgb(169, 255, 104) none
    # $yellow
    link: rgb(255, 255, 15) none
    # $subtext0
    pruning: rgb(165, 173, 203) none Italic
#
### PREVIEW
#
    # fg:$text bg:$mantle
    preview_title: rgb(192, 202, 245) rgb(33, 34, 44)
    # fg:$text bg:$mantle
    preview: rgb(192, 202, 245) rgb(33, 34, 44)
    # fg:$overlay0
    preview_line_number: rgb(68, 71, 90) none
#
### MATCH
#
    char_match: rgb(255, 255, 15) rgb(73, 77, 100) Bold Italic
    content_match: rgb(255, 255, 15) rgb(73, 77, 100) Bold Italic
    preview_match: rgb(255, 255, 15) rgb(73, 77, 100) Bold Italic

    # children count
    # fg:$yellow bg:none
    count: rgb(255, 255, 15) none
    sparse: rgb(236, 95, 103) none
    content_extract: rgb(236, 95, 103) none Italic
#
### GIT
#
    git_branch: rgb(247, 144, 0) none
    git_insertions: rgb(247, 144, 0) none
    git_deletions: rgb(247, 144, 0) none
    git_status_current: rgb(247, 144, 0) none
    git_status_modified: rgb(247, 144, 0) none
    git_status_new: rgb(247, 144, 0) none Bold
    git_status_ignored: rgb(247, 144, 0) none
    git_status_conflicted: rgb(247, 144, 0) none
    git_status_other: rgb(247, 144, 0) none
    staging_area_title: rgb(247, 144, 0) none
#
### FLAG
#
    flag_label: rgb(236, 95, 103) none
    flag_value: rgb(236, 95, 103) none Bold
#
### STATUS
#
    # fg:none #bg:$mantle
    status_normal: rgb(84, 95, 137) rgb(33, 34, 44)
    # fg:$red bg:$mantle
    status_italic: rgb(236, 95, 103) rgb(33, 34, 44) Italic
    # fg:$maroon bg:$mantle
    status_bold: rgb(189, 147, 249) rgb(33, 34, 44) Bold
    # fg:$maroon bg:$mantle
    status_ellipsis: rgb(189, 147, 249) rgb(33, 34, 44) Bold
    # fg:$text bg:$red
    status_error: rgb(33, 34, 44) rgb(236, 95, 103)
    # fg:$maroon bg:$mantle
    status_job: rgb(189, 147, 249) rgb(40, 38, 37)
    # fg:$maroon bg:$mantle
    status_code: rgb(189, 147, 249) rgb(33, 34, 44) Italic
    # fg:$maroon bg:$mantle
    mode_command_mark: rgb(189, 147, 249) rgb(33, 34, 44) Bold
#
### HELP
#
    # fg:$text
    help_paragraph: rgb(192, 202, 245) none
    # fg:$red
    help_headers: rgb(236, 95, 103) none Bold
    # fg:$peach
    help_bold: rgb(247, 144, 0) none Bold
    # fg:$yellow
    help_italic: rgb(255, 255, 15) none Italic
    # fg:green bg:$surface0
    help_code: rgb(169, 255, 104) rgb(54, 58, 79)
    # fg:$overlay0
    help_table_border: rgb(68, 71, 90) none
#
### HEX
#
    # fg:$text
    hex_null: rgb(192, 202, 245) none
    # fg:$peach
    hex_ascii_graphic: rgb(247, 144, 0) none
    # fg:$green
    hex_ascii_whitespace: rgb(169, 255, 104) none
    # fg: teal
    hex_ascii_other: rgb(241, 250, 140) none
    # fg: red
    hex_non_ascii: rgb(236, 95, 103) none

    # fg:$text bg:$red
    file_error: rgb(192, 202, 245) none
#
### PURPOSE
#
    purpose_normal: none none
    purpose_italic: rgb(169, 90, 127) none Italic
    purpose_bold: rgb(169, 90, 127) none Bold
    purpose_ellipsis: none none
#
### SCROLLBAR
#
    # fg:$surface0
    scrollbar_track: rgb(54, 58, 79) none
    # fg:$surface1
    scrollbar_thumb: rgb(40, 42, 54) none
#
### GOODTOBAD
#
    good_to_bad_0: rgb(169, 255, 104) none
    good_to_bad_1: rgb(241, 250, 140) none
    good_to_bad_2: rgb(137, 221, 255) none
    good_to_bad_3: rgb(130, 170, 255) none
    good_to_bad_4: rgb(138, 173, 244) none
    good_to_bad_5: rgb(138, 162, 247) none
    good_to_bad_6: rgb(198, 160, 246) none
    good_to_bad_7: rgb(247, 144, 0) none
    good_to_bad_8: rgb(189, 147, 249) none
    good_to_bad_9: rgb(236, 95, 103) none
}
# syntax_theme: GitHub
syntax_theme: SolarizedDark
# syntax_theme: SolarizedLight
# syntax_theme: MochaDark
# syntax_theme: OceanDark
# syntax_theme: OceanLight

